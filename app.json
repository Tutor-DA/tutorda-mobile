{
  "expo": {
    "name": "TutorDA.org",
    "slug": "tutorda-math-app",
      "scheme": "tutorda-math-app",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "android",
      "ios"
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc",
      "bundleIdentifier": "com.anonymous.tutordamathapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": true
      }
    },
    "web": {
      "bundler": "webpack",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
        [
            "expo-font",
            {
                "fonts": [
                    {
                        "name": "Poppins-Regular",
                        "file": "./assets/fonts/Poppins-Regular.ttf"
                    },
                    {
                        "name": "Poppins-Bold",
                        "file": "./assets/fonts/Poppins-Bold.ttf"
                    },
                    {
                        "name": "Inter-Regular",
                        "file": "./assets/fonts/Inter-Regular.ttf"
                    }
                ]
            }
        ],
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "launchMode": "mostRecent"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "android": {
      "package": "com.anonymous.tutordamathapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "15172dbb-2396-4c8d-a441-1607dfc92683"
      }
    },
    "owner": "tutorda"
  }
}
