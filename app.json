{
  "expo": {
    "name": "TutorDA.org",
    "slug": "tutorda-math-app",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "tutorda",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },   
    "web": {
      "bundler": "webpack",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "android": {
      "package": "com.anonymous.tutordamathapp"
    }
  }
}
